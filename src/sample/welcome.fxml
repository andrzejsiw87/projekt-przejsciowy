<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="388.0" prefWidth="1044.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.WelcomeController">
    <bottom>
        <ButtonBar BorderPane.alignment="CENTER">
            <buttons>
                <Button fx:id="cancelButton" onAction="#cancelPressed" text="Anuluj" />
                <Button fx:id="startButton" onAction="#nextPressed" text="Dalej" />
            </buttons>
        </ButtonBar>
    </bottom>
    <center>
        <HBox prefHeight="357.0" prefWidth="1125.0" BorderPane.alignment="CENTER">
            <children>
                <Label fx:id="welcomeText" alignment="CENTER" prefHeight="324.0" prefWidth="973.0" text="Praca na zaliczenie projektu przejściowego na przykładnie zadania 2. Przygotowana przez: Andrzej Siwek">
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
               <font>
                  <Font size="17.0" />
               </font>
                </Label>
            </children>
        </HBox>
    </center>
</BorderPane>
